#: app.py:94
#, python-format
msgid "The content from \"%(source)s\" does not appear to be a valid PDF."
msgstr "El contenido de \"%(source)s\" no parece ser un PDF válido."

#: app.py:97
#, python-format
msgid "The PDF from \"%(source)s\" is password protected and cannot be processed."
msgstr "El PDF de \"%(source)s\" está protegido por contraseña y no puede ser procesado."

#: app.py:110
#, python-format
msgid ""
"No extractable text found in the PDF \"%(source)s\". It might be an image"
" file, empty, or contain only graphics."
msgstr "No se encontró texto extraíble en el PDF \"%(source)s\". Podría ser un archivo de imagen, estar vacío o contener solo gráficos."

#: app.py:113
#, python-format
msgid "Invalid or corrupted PDF file from \"%(source)s\": %(error)s"
msgstr "Archivo PDF inválido o corrupto de \"%(source)s\": %(error)s"

#: app.py:119
#, python-format
msgid "Invalid or unsupported PDF format from \"%(source)s\": %(error)s"
msgstr "Formato PDF inválido o no soportado de \"%(source)s\": %(error)s"

#: app.py:121
#, python-format
msgid "Unexpected error processing the PDF from \"%(source)s\": %(error)s"
msgstr "Error inesperado al procesar el PDF de \"%(source)s\": %(error)s"

#: app.py:134
#, python-format
msgid ""
"No main text found at \"%(url)s\". The page might be empty, heavily "
"reliant on JavaScript, or the main content was removed during parsing."
msgstr "No se encontró texto principal en \"%(url)s\". La página podría estar vacía, depender mucho de JavaScript o el contenido principal fue eliminado durante el análisis."

#: app.py:138
#, python-format
msgid "Error parsing HTML content from URL \"%(url)s\": %(error)s"
msgstr "Error al analizar el contenido HTML de la URL \"%(url)s\": %(error)s"

#: app.py:155
msgid "input"
msgstr "entrada"

#: app.py:165
msgid "Invalid file type for upload."
msgstr "Tipo de archivo inválido para subir."

#: app.py:165
msgid "Please upload a PDF."
msgstr "Por favor, sube un PDF."

#: app.py:176
msgid "Invalid URL format."
msgstr "Formato de URL inválido."

#: app.py:195
#, python-format
msgid "The content at the URL is too large (larger than %(max_size)sMB)."
msgstr "El contenido en la URL es demasiado grande (mayor que %(max_size)sMB)."

#: app.py:208
#, python-format
msgid ""
"The URL does not point to a supported PDF or HTML page (Content-Type: "
"%(ctype)s)."
msgstr "La URL no apunta a una página PDF o HTML soportada (Content-Type: %(ctype)s)."

#: app.py:210
#, python-format
msgid "Timeout (%(sec)ss) when trying to access the URL: %(url)s"
msgstr "Tiempo de espera agotado (%(sec)ss) al intentar acceder a la URL: %(url)s"

#: app.py:214
#, python-format
msgid "Network error accessing the URL %(url)s: %(error)s"
msgstr "Error de red al acceder a la URL %(url)s: %(error)s"

#: app.py:218
msgid "No PDF file was uploaded or valid URL provided."
msgstr "No se subió ningún archivo PDF o no se proporcionó una URL válida."

#: app.py:227
#, python-format
msgid ""
"Could not extract text from \"%(source)s\" (empty result after "
"processing)."
msgstr "No se pudo extraer texto de \"%(source)s\" (resultado vacío después del procesamiento)."

#: app.py:237
msgid ""
"An unexpected error occurred during processing. Check server logs for "
"details."
msgstr "Ocurrió un error inesperado durante el procesamiento. Revisa los logs del servidor para detalles."

#: app.py:250
msgid ""
"Error: No extracted text found in the current session for download. "
"Please try processing a PDF or URL again."
msgstr "Error: No se encontró texto extraído en la sesión actual para descargar. Por favor, intenta procesar un PDF o URL nuevamente."

#: app.py:253
msgid "Invalid download format."
msgstr "Formato de descarga inválido."

#: app.py:266 templates/results.html:28
msgid "Extracted Text"
msgstr "Texto Extraído"

#: app.py:286
msgid "Error generating the file for download."
msgstr "Error al generar el archivo para descargar."

